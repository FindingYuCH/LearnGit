Magix.tmpl("app/views/wxrobot",'<div class=wxrobot-bar bx-tmpl="sourceId" bx-datakey="sourceId"> <div class="wxrobot-trigger iconfont-pub" mx-click="show{sourceId:{{sourceId}}}">&#xe62d;</div> </div>'),KISSY.add("app/views/wxrobot",function(i,e,t,a){t.all;return e.extend({init:function(){var i=!1;window.AW&&window.AW.init({sourceId:464,bizCode:"PCMaMaAnyWhereWindow",logoWidth:30,onRendered:function(){i&&window.AW.openDialog({isFirstAnswer:!0}),i=!0}})},render:function(){var i=this.location,e=i.pathname,t=a.getSourceId(e);this.setViewPagelet({sourceId:t})},locationChange:function(i){i.changed.isPathname()&&this.render()},events:{click:{show:function(i){window.AW&&window.AW.refresh({sourceId:i.params.sourceId})}}}})},{requires:["mxext/view","node","app/util/util","app/util/datepicker/datepicker","app/util/dialog/dialog","app/util/format/format","app/util/globaltip/globaltip","app/util/robot/sourceid","app/util/spmlog/spmlog","app/util/mathextend/mathextend","app/util/tooltip/tooltip","app/util/widgetds/widgetds","app/util/rank/rank","app/util/reporttip/reporttip","app/util/vcode/vcode","app/util/pagination/index","app/util/fields/fields","app/util/mouseevent/index","magix/vframe","magix/vom","magix/router","brix/gallery/datepicker/index","brix/gallery/calendar/index","brix/gallery/dialog/index","app/util/spmlog/pathmap"]});